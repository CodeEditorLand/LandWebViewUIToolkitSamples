{
	"name": "hello-world",
	"displayName": "hello-world",
	"version": "0.0.1",
	"private": false,
	"description": "A hello world sample extension to show off the Webview UI Toolkit for Visual Studio Code.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "webpack",
		"package": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run package",
		"watch": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "hello-world.showHelloWorld",
				"title": "Hello World: Show"
			}
		]
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "^1.2.2"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@types/vscode-webview": "^1.57.0",
		"glob": "^8.0.3",
		"ts-loader": "^9.4.1",
		"typescript": "^4.8.4",
		"typescript-esbuild": "0.4.5",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
