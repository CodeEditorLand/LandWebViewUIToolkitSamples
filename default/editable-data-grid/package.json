{
	"name": "editable-data-grid",
	"displayName": "editable-data-grid",
	"version": "0.0.1",
	"description": "A sample extension that demonstrates a solution for making editable data grids.",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "node ./esbuild.js",
		"lint": "eslint src --ext ts",
		"package": "NODE_ENV=production node ./esbuild.js",
		"vscode:prepublish": "npm run package",
		"watch": "node ./esbuild.js --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "editable-data-grid.showEditableDataGrid",
				"title": "Editable Data Grid: Show"
			}
		]
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "^1.2.2"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"esbuild": "^0.16.10",
		"eslint": "^8.26.0",
		"glob": "^8.0.3",
		"prettier": "^2.8.1",
		"typescript": "^4.8.4"
	},
	"engines": {
		"vscode": "^1.75.0"
	}
}
