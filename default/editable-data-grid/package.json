{
  "name": "editable-data-grid",
  "displayName": "editable-data-grid",
  "description": "A sample extension that demonstrates a solution for making editable data grids.",
  "version": "0.0.1",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editable-data-grid.showEditableDataGrid",
        "title": "Editable Data Grid: Show"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "node ./esbuild.js --watch"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/node": "20.x",
    "@types/vscode": "1.85.0",
    "@types/vscode-webview": "1.57.4",
    "esbuild": "0.19.9",
    "glob": "10.3.10",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "1.4.0"
  }
}
