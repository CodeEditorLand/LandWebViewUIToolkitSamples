{
  "name": "editable-data-grid",
  "displayName": "editable-data-grid",
  "version": "0.0.1",
  "private": false,
  "description": "A sample extension that demonstrates a solution for making editable data grids.",
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
    "vscode:prepublish": "npm run package",
    "watch": "node ./esbuild.js --watch"
  },
  "contributes": {
    "commands": [
      {
        "command": "editable-data-grid.showEditableDataGrid",
        "title": "Editable Data Grid: Show"
      }
    ]
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@types/vscode-webview": "^1.57.0",
    "esbuild": "^0.16.10",
    "glob": "^8.0.3",
    "typescript": "^4.8.4",
    "typescript-esbuild": "0.3.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
