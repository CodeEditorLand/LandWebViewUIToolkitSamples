{
	"name": "notepad",
	"displayName": "notepad",
	"version": "0.0.1",
	"private": false,
	"description": "A sample extension that showcases the Webview UI Toolkit for VS Code alongside the VS Code TreeView and Webview API",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandWebViewUIToolkitSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandWebViewUIToolkitSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandWebViewUIToolkitSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "node ./esbuild.js",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "notepad.createNote",
				"icon": "$(add)",
				"title": "Notepad: Create a note"
			},
			{
				"command": "notepad.deleteNote",
				"icon": "$(trash)",
				"title": "Notepad: Delete note"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "notepad.deleteNote",
					"group": "inline"
				}
			],
			"view/title": [
				{
					"command": "notepad.createNote",
					"group": "navigation@1"
				}
			]
		},
		"views": {
			"notepad": [
				{
					"id": "notepad.notesList",
					"name": "My Notes",
					"type": "tree",
					"visibility": "visible"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "$(notebook)",
					"id": "notepad",
					"title": "Notepad"
				}
			]
		},
		"viewsWelcome": [
			{
				"contents": "No notes yet!\n[Create a note](command:notepad.createNote)",
				"view": "notepad.notesList"
			}
		]
	},
	"dependencies": {
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "^8.0.0",
		"@types/node": "16.x",
		"@types/uuid": "^9.0.0",
		"esbuild": "^0.16.10",
		"esbuild-plugin-copy": "^2.0.1",
		"glob": "^8.0.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
