{
	"name": "hello-world-react-vite",
	"displayName": "hello-world-react-vite",
	"version": "0.0.1",
	"private": false,
	"description": "A hello world sample extension to demonstrate using React + Vite with the Webview UI Toolkit for VS Code.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandWebViewUIToolkitSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandWebViewUIToolkitSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandWebViewUIToolkitSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build:webview": "cd webview-ui && npm run build",
		"compile": "tsc -p ./",
		"install:all": "npm install && cd webview-ui && npm install",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"start:webview": "cd webview-ui && npm run start"
	},
	"contributes": {
		"commands": [
			{
				"command": "hello-world.showHelloWorld",
				"title": "Hello World (React + Vite): Show"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.7",
		"glob": "10.3.12"
	},
	"publishConfig": {
		"access": "public"
	}
}
