{
	"name": "hello-world-svelte",
	"displayName": "hello-world-svelte",
	"version": "0.0.1",
	"private": "false",
	"description": "A hello world sample extension to demonstrate using Svelte with the Webview UI Toolkit for VS Code.",
	"homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Nikola Hristov",
		"email": "nikola@nikolahristov.tech",
		"url": "https://nikolahristov.tech"
	},
	"publisher": "playform",
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"build:webview": "cd webview-ui && npm run build",
		"compile": "tsc -p ./",
		"install:all": "npm install && cd webview-ui && npm install",
		"lint": "eslint src --ext ts",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"start:webview": "cd webview-ui && npm run dev",
		"pretest": "npm run compile && npm run lint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "hello-world.showHelloWorld",
				"title": "Hello World (Svelte): Show"
			}
		]
	},
	"devDependencies": {
		"@types/glob": "8.1.0",
		"@types/node": "20.10.4",
		"@types/vscode": "1.85.0",
		"@typescript-eslint/eslint-plugin": "6.13.2",
		"@typescript-eslint/parser": "6.13.2",
		"glob": "10.3.10",
		"typescript": "5.3.3",
		"typescript-esbuild": "0.3.2",
		"vscode-test": "1.5.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
