{
	"name": "hello-world-vue",
	"displayName": "hello-world-vue",
	"version": "0.0.1",
	"private": false,
	"description": "A hello world sample extension to demonstrate using Vue with the Webview UI Toolkit for VS Code.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"build:webview": "cd webview-ui && npm run build",
		"compile": "tsc -p ./",
		"install:all": "npm install && cd webview-ui && npm install",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"start:webview": "cd webview-ui && npm run start",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "hello-world.showHelloWorld",
				"title": "Hello World (Vue): Show"
			}
		]
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"glob": "^7.1.6",
		"typescript": "^4.1.3",
		"typescript-esbuild": "0.4.5",
		"vscode-test": "^1.5.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
