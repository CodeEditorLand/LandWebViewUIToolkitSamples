{
	"name": "hello-world",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "node ./scripts/build-react-no-split.js",
		"eject": "react-scripts eject",
		"start": "EXTEND_ESLINT=true react-scripts start",
		"test": "EXTEND_ESLINT=true react-scripts test"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"rules": {
			"no-unused-expressions": "off"
		}
	},
	"dependencies": {
		"@vscode/webview-ui-toolkit": "1.3.1",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "20.9.2",
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"@types/vscode-webview": "1.57.4",
		"react-scripts": "5.0.1",
		"rewire": "7.0.0",
		"typescript": "5.2.2"
	}
}
